proc            /proc           proc    defaults          0       0
PARTUUID=79ae1212-01  /boot           vfat    defaults          0       2
PARTUUID=79ae1212-02  /               ext4    defaults,noatime  0       1
/dev/sda   /mnt/ssd1   ext4   defaults   0   2
/dev/sdb   /mnt/ssd2   ext4   defaults   0   2
# a swapfile is not a swap partition, no line here
#   use  dphys-swapfile swap[on|off]  for that
tmpfs /var/tmp tmpfs defaults,noatime,nosuid,nodev,noexec,mode=0755,size=1M 0 0